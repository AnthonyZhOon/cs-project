{
	"name": "cs-project",
	"version": "0.0.1",
	"private": "true",
	"publishConfig": {
		"access": "restricted"
	},
	"engines": {
		"node": ">=22",
		"pnpm": ">=10"
	},
	"scripts": {
		"dev": "prisma generate && next",
		"build": "prisma generate && next build",
		"start": "next start",
		"test": "vitest",
		"lint": "eslint --cache",
		"format": "prettier -w .",
		"generate-prisma": "prisma generate",
		"db:start": "docker compose up -d",
		"db:seed": "prisma migrate deploy && tsx -e 'import prisma from \"./lib/prisma\"; import seed from \"./prisma/seed\"; seed(prisma)'",
		"db:drop": "tsx prisma/drop-tables.ts",
		"db:stop": "docker compose down"
	},
	"license": "MIT",
	"dependencies": {
		"@next/env": "^15.4.4",
		"@prisma/client": "^6.13.0",
		"next": "^15.4.4",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-markdown": "^10.1.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.32.0",
		"@next/eslint-plugin-next": "^15.4.4",
		"@tailwindcss/postcss": "^4.1.11",
		"@types/node": "~22.15.21",
		"@types/pg": "^8.15.5",
		"@types/react": "^19.1.9",
		"eslint": "^9.32.0",
		"eslint-config-next": "^15.4.4",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-only-warn": "^1.1.0",
		"eslint-plugin-perfectionist": "^4.15.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-unicorn": "^60.0.0",
		"globals": "^16.3.0",
		"nanoid": "^5.1.5",
		"pg": "^8.16.3",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prisma": "^6.13.0",
		"prisma-dbml-generator": "^0.12.0",
		"tailwindcss": "^4.1.11",
		"tsx": "^4.20.3",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.38.0",
		"vercel": "^44.6.4",
		"vite": "^7.1.2",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4"
	}
}
